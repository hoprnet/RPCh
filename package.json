{
  "name": "@rpch/monorepo",
  "version": "0.0.0",
  "license": "LGPL-3.0",
  "private": true,
  "workspaces": [
    "configs/*",
    "packages/*",
    "apps/*",
    "devkit/*"
  ],
  "scripts": {
    "build": "turbo run build --no-cache",
    "dev": "turbo run dev",
    "format": "turbo run format",
    "format:fix": "turbo run format -- --check=false --write=true",
    "lint": "turbo run lint --no-cache",
    "depcheck": "check-dependency-version-consistency",
    "test": "turbo run test --",
    "test:e2e": "turbo run start --filter @rpch/e2e"
  },
  "dependencies": {},
  "devDependencies": {
    "@changesets/cli": "^2.25.2",
    "check-dependency-version-consistency": "^3.0.3",
    "turbo": "^1.6.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "yarn@1.22.19"
}